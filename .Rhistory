sw1$time <- as.Date(sw1$time, '%Y-%m-%d')
sw1<- sw1[, c(2,3)]
fd_sw1 <- FordLabosier2017(sw1)
fd_sw1
de_tha.swc_series_era5 <- fd_sw1[[1]]
de_tha.fd_info_era5 <- fd_sw1[[2]]
ggplot(de_tha.swc_series_era5, aes(x = Date), inherit.aes = F)+
geom_bar(aes(weight = is.fd))+
geom_line(col = 'red', aes(y = SWC/50))+
scale_y_continuous(breaks=c(0,1), sec.axis = sec_axis(trans = ~ (.*50), name = 'SWC'))+
theme_bw() + labs(x = 'Date', y = 'Flash Drought')+
theme(strip.background = element_rect(fill = 'white', linetype = 2))
de_tha.fd_info_era5
